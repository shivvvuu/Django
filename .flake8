[flake8]
ignore = C101,D403,F403,F405,I100,W503,W504,W292, W605, F821
exclude =
    .git,
    .mypy_cache,
    __pycache__,
    venv
filename = *.py
accept-encodings = utf-8
inline-quotes = single
max-complexity =12
max-linenumber = 500
max-line-length = 119
multiline-quotes = double
per-file-ignores =
    core/project/settings/templates/settings.dev.py:F821
